json = """
{
  "pipeline":[
    # Path to LAS FILE,
    {
      "type":"writers.gdal",
      "filename":"NEON_TEAK_DSM.tif",
      "dimension":"Elevation",
      "data_type":"float",
      "output_type":"max",
      "resolution":1.0,
      "radius": 0.5
    }
  ]
}"""

import pdal
pipeline = pdal.Pipeline(json)
pipeline.validate()
pipeline.execute()